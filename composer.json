{
  "name": "paulocoelho/laravel-scaffold-plus",
  "description": "Geração completa de estruturas Laravel: Model, Controller, Service, Repository, Provider, Requests e Policy com exemplos de uso.",
  "keywords": ["laravel", "scaffold", "artisan", "generator"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Paulo Coelho",
      "email": "paulomedinabr01@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "illuminate/support": "^10.0 || ^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "PauloCoelho\\LaravelScaffoldPlus\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PauloCoelho\\LaravelScaffoldPlus\\ScaffoldServiceProvider"
      ]
    }
  },
  "suggest": {
    "laravel/framework": "Necessário para integração com o Laravel (>=10.0)"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
